<%= render 'books/list', book: @book %>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.user.name %></td>
        <td><%= link_to "Show", book_path(book.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>